<style>
  #notifications pre:nth-child(odd) {
    background-color: rgba(0, 200, 0, 0.1);
  }
  #notifications pre:nth-child(n+45) {
    background-color: rgba(200, 0, 0, 0.2);
  }
</style>
<details>
  <h1>params-logger</h1>
  <h2>History:</h2>
  <pre id="history"></pre>
  <h2>Path:</h2>
  <pre id="Path">
    <%= @path %>
  </pre>
  <h2>Params:</h2>
  <pre id="params">
    <%= @params %>
  </pre>
  <h2>Headers:</h2>
  <code id="headers">
    <% @headers_filtered.each do |header| %>
      <%= header %><br>
    <% end %>
  </code>
</details>
<h2>Notifications:</h2>
<div id="notifications"></div>
